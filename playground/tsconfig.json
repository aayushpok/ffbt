{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["DOM", "ES5", "ScriptHost", "ES2015", "ES2015.Iterable"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  },
  "exclude": [
    "src-old",
    "node_modules"
  ]
}
